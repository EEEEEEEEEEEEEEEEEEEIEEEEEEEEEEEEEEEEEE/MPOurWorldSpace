.page-header {
  align-items: center;
  display: flex;
  justify-content: space-between;
  padding: 0 40rpx;
}

.sub {
  margin: 10px;
}

.title, .title-sub {
  display: block;
}

.title {
  font-size: 24px;
  font-weight: bold;
  line-height: 32px;
  overflow: hidden;
  margin-bottom: 6px;
}

.title-sub {
  color: #999;
  font-family: Arial, Helvetica, sans-serif;
  font-size: 14px;
  overflow: hidden;
}
